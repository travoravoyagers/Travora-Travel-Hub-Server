generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id           String   @id @default(cuid())
  name         String?
  email        String   @unique
  passwordHash String
  profileImage String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
